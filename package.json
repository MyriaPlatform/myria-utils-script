{
  "name": "myria-utils-scripts",
  "version": "0.0.1",
  "description": "myria-utils-scripts",
  "author": "myria-core-team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "env:local": "./scripts/replace_json_file.sh './configs/config.local.json' './configs/config.json'",
    "env:dev": "./scripts/replace_json_file.sh './configs/config.dev.json' './configs/config.json'",
    "env:stg": "./scripts/replace_json_file.sh './configs/config.staging.json' './configs/config.json'",
    "env:prod": "./scripts/replace_json_file.sh './configs/config.prod.json' './configs/config.json'",
    "env:test": "./scripts/replace_json_file.sh './configs/config.test.json' './configs/config.json'",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "IS_LOCAL_MACHINE=true nest start",
    "start:local": "migration:run:local & npm run env:local & npm run build & IS_LOCAL_MACHINE=true nest start --debug --watch",
    "start:dev": "npm run env:dev & npm run build & nest start",
    "start:stg": "npm run env:stg & npm run build & nest start",
    "start:prod": "npm run env:prod & npm run build & nest start",
    "start:test": "npm run env:test & npm run build & nest start",
    "start:debug": "nest start --debug --watch",
    "test:setup": "npm run env:test",
    "test": "npm run test:setup & IS_LOCAL_MACHINE=true && jest",
    "test:local": "IS_LOCAL_MACHINE=true npm run test",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration:ci": "npm run test:setup && jest --runInBand --bail --reporters default jest-stare --forceExit",
    "test:integration": "IS_INTEGRATION_TEST=true IS_LOCAL_MACHINE=true jest --runInBand --bail --reporters default jest-stare --forceExit",
    "prepare": "husky install",
    "transfer:erc20:local": "npm run env:local & ts-node ./src/scripts/transfer-erc20.script.ts",
    "transfer:erc20:dev": "npm run env:dev & ts-node ./src/scripts/transfer-erc20.script.ts",
    "transfer:erc20:stg": "npm run env:stg & ts-node ./src/scripts/transfer-erc20.script.ts",
    "transfer:erc20:prod": "npm run env:prod & ts-node ./src/scripts/transfer-erc20.script.ts",
    "transfer:erc721:local": "npm run env:local & ts-node ./src/scripts/transfer-erc721.script.ts",
    "transfer:erc721:dev": "npm run env:dev & ts-node ./src/scripts/transfer-erc721.script.ts",
    "transfer:erc721:stg": "npm run env:stg & ts-node ./src/scripts/transfer-erc721.script.ts",
    "transfer:erc721:prod": "npm run env:prod & ts-node ./src/scripts/transfer-erc721.script.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.5",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@starkware-industries/starkware-crypto-utils": "^0.0.2",
    "aws-sdk": "^2.1234.0",
    "axios": "^0.27.2",
    "cache-manager": "^3.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cli-table": "^0.3.11",
    "csv-parse": "^5.3.3",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "enc-utils": "^3.0.0",
    "ethers": "^5.6.9",
    "express-http-context": "^1.2.4",
    "form-data": "^4.0.0",
    "helmet": "^5.1.0",
    "md5": "^2.3.0",
    "myria-core-sdk": "^0.0.167-v10",
    "newrelic": "^8.13.2",
    "on-headers": "^1.0.2",
    "pg": "^8.7.3",
    "query-string": "^7.1.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sqlstring": "^2.3.3",
    "sqs-consumer": "^5.7.0",
    "swagger-ui-express": "^4.4.0",
    "webpack": "^5.72.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/bn.js": "^5.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.18.2",
    "@types/sqlstring": "^2.3.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-more": "^1.0.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "28.0.3",
    "jest-stare": "^2.4.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "testTimeout": 180000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
